@startuml "Diagramme de Classe"

class "Acheteur" {
    +id: int
    +nom: string
    +email: string
    +chercherBiens(): List<Bien>
    +afficherBiens(): void
    +contacterAdministrateur(): void
}

class "Administrateur" {
    +id: int
    +nom: string
    +email: string
    +CRUD_Biens(): void
    +gererContacts(): void
}

class "Bien" {
    +id: int
    +titre: string
    +description: string
    +prix: float
    +type: string
}

class "Contact" {
    +id: int
    +nom: string
    +email: string
    +message: string
}

Acheteur --> Bien : "cherche"
Acheteur --> Contact : "contacter"
Administrateur --> Bien : "gère"
Administrateur --> Contact : "gère"

@enduml
